<div class="col-xxl-2 col-xl-2 col-lg-2 col-md-12 col-sm-12 col-12 users">
    <strong>Users <small>(@RoomUsers.Count)</small></strong>
    <ul>
        @foreach (var user in RoomUsers)
        {
            <li class="user @(user == Username ? "you" : "")">@user</li>
        }
    </ul>
</div>

@code {

    [Parameter]
    public List<string> RoomUsers { get; set; }

    [Parameter]
    public string Username { get; set; }

}
